            "content": "<template>\r\n  <div class=\"modal\" v-if=\"modal\">\r\n    <!-- <div class=\"modal__info\">\r\n      <button @click=\"close\">關閉</button>\r\n      <p>Some text in the Modal..</p>\r\n    </div> -->\r\n    <div class=\"modal__info\">\r\n      <button @click=\"close\">關閉</button>\r\n      <div class=\"modal__top\">\r\n        <div class=\"modal__top__info\">\r\n          <div class=\"modal__top__line\"></div>\r\n          <div class=\"modal__top__flight\">\r\n            <div class=\"modal__top__flight__left\">{{ data.flight.departure }}</div>\r\n            <div>to</div>\r\n            <div class=\"modal__top__flight__right\">{{ data.flight.destination }}</div>\r\n          </div>\r\n          <div class=\"modal__top__line\"></div>\r\n        </div>\r\n        <div class=\"modal__top__info__bar\">\r\n          <div class=\"modal__top__info__bar__left\">\r\n            <div>Passenger</div>\r\n            <div>{{ data.passenger }}</div>\r\n          </div>\r\n          <div class=\"modal__top__info__bar__right\">\r\n            <div>Date</div>\r\n            <div>{{ data.departureTime }}</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"modal__top__info__bar\">\r\n          <div class=\"modal__top__info__bar__left\">\r\n            <div>班機</div>\r\n            <div>{{ data.flight.flightNumber }}</div>\r\n          </div>\r\n          <div class=\"modal__top__info__bar__right\">\r\n            <div>Gate/Seat</div>\r\n            <div>{{ data.gateSeat }}</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <div class=\"modal__doash\">\r\n        <div class=\"modal__doash__top\"></div>\r\n        <div class=\"modal__doash__bottom\"></div>\r\n      </div>\r\n      <div class=\"modal__bottom\">\r\n        <div>Qr Code</div>\r\n        <div class=\"modal__bottom__qrcode\">\r\n          <img :src=\"data.qrcode\" alt=\"\" class=\"modal__bottom__qrcode__img\" loading=\"lazy\"/>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script setup>\r\nimport { defineProps } from 'vue'\r\n\r\nconst emit = defineEmits(['close'])\r\n\r\nconst close = () => {\r\n  emit('close')\r\n}\r\nconst props = defineProps({\r\n  modal: {\r\n    type: Boolean,\r\n    default: false\r\n  },\r\n  data: {\r\n    type: Object\r\n  }\r\n})\r\n</script>\r\n<style scoped lang=\"scss\">\r\n.modal {\r\n  left: 0;\r\n  position: fixed;\r\n  z-index: 1;\r\n  top: 0;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: auto;\r\n  background-color: rgb(0, 0, 0);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background-color: rgba(0, 0, 0, 0.4);\r\n  &__info {\r\n    // background-color: white;\r\n    // height: 70vh;\r\n    // display: flex;\r\n    // padding: 0 2rem;\r\n    // max-width: 1280px;\r\n    // margin: 0 auto;\r\n    // padding: 2rem;\r\n    // font-weight: normal;\r\n    // width: 47rem;\r\n    // align-items: center;\r\n    // border-radius: 2%;\r\n  }\r\n\r\n  &__top {\r\n    border-top-left-radius: 1.5rem;\r\n    border-top-right-radius: 1.5rem;\r\n    padding: 1rem 2rem;\r\n    background-color: white;\r\n    width: 31em;\r\n\r\n    &__info {\r\n      display: flex;\r\n      align-items: center;\r\n      justify-content: center;\r\n      padding: 10px 0;\r\n\r\n      &__bar {\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: space-between;\r\n        width: 100%;\r\n        padding: 0.3rem;\r\n\r\n        &__left {\r\n          display: flex;\r\n          flex-direction: column;\r\n          flex-grow: 1;\r\n          width: 500px;\r\n        }\r\n\r\n        &__right {\r\n          display: flex;\r\n          flex-direction: column;\r\n          flex-grow: 1;\r\n          width: 400px;\r\n        }\r\n      }\r\n    }\r\n\r\n    &__line {\r\n      width: 100%;\r\n      height: 0.3px;\r\n      border-style: solid;\r\n      border: 1px solid rgb(202, 202, 202);\r\n    }\r\n\r\n    &__flight {\r\n      display: flex;\r\n      width: 30vw;\r\n      align-items: center;\r\n      justify-content: space-around;\r\n\r\n      &__left,\r\n      &__right {\r\n        font-size: 19px;\r\n        text-align: center;\r\n      }\r\n    }\r\n  }\r\n  &__doash {\r\n    border-style: dashed;\r\n    border-width: 2px;\r\n    position: relative;\r\n    color: #f5eeee;\r\n\r\n    &__top {\r\n      position: absolute;\r\n      border-radius: 9999px;\r\n      width: 2rem;\r\n      height: 2rem;\r\n      background-color: rgb(153 153 153);\r\n      top: -1.25rem;\r\n      right: -16px;\r\n    }\r\n\r\n    &__bottom {\r\n      position: absolute;\r\n      border-radius: 9999px;\r\n      width: 2rem;\r\n      height: 2rem;\r\n      background-color: rgb(153 153 153);\r\n\r\n      bottom: -15px;\r\n      left: -16px;\r\n    }\r\n  }\r\n  &__bottom {\r\n    border-bottom-left-radius: 1.5rem;\r\n    border-bottom-right-radius: 1.5rem;\r\n    padding: 1rem;\r\n    background-color: white;\r\n    width: 31em;\r\n\r\n    &__qrcode {\r\n      &__img {\r\n        width: 100%;\r\n        height: auto;\r\n      }\r\n    }\r\n  }\r\n  //   &__doash {\r\n  //     border-style: dashed;\r\n  //     border-width: 2px;\r\n  //     position: relative;\r\n  //     color: #f5eeee;\r\n  //   }\r\n}\r\n</style>\r\n"
